{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/payment/checkout/index.ts": {
      "maxDuration": 60
    },
    "api/vpn/create.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/vpn/poll-activations",
      "schedule": "0 0 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
